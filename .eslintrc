{
    "env": {
        "browser": true,
        "node": true,
        "es6": true,
    },
    "ecmaFeatures": {
        "modules": true
    },
    "parser": "babel-eslint",
    "rules": {
        "no-shadow": 0,
        "space-unary-ops": 0,
        "strict": 0,
        "quotes": [2, "single"],
        "eol-last": [0],
        "no-mixed-requires": [0],
        "no-underscore-dangle": [0],
        "no-alert": [0],
    }
}
